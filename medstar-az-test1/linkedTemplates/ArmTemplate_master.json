{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "medstar-az-test1"
		},
		"Oracle_v500_yb1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Oracle_v500_yb1'"
		},
		"az_blob_source1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_blob_source1'"
		},
		"pmh_azdf_yb2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'pmh_azdf_yb2'"
		},
		"pmh_azdf_yb2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'pmh_azdf_yb2'"
		},
		"stroke_reg_yb1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'stroke_reg_yb1'"
		},
		"stroke_reg_yb1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'stroke_reg_yb1'"
		},
		"pmh_azdf_yb2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "MEDSTAR\\YXX105"
		},
		"stroke_reg_yb1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "MEDSTAR\\YXX105"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "medstar-az-test1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"az_blob_source1_connectionString": {
						"value": "[parameters('az_blob_source1_connectionString')]"
					},
					"Oracle_v500_yb1_connectionString": {
						"value": "[parameters('Oracle_v500_yb1_connectionString')]"
					},
					"pmh_azdf_yb2_connectionString": {
						"value": "[parameters('pmh_azdf_yb2_connectionString')]"
					},
					"pmh_azdf_yb2_password": {
						"value": "[parameters('pmh_azdf_yb2_password')]"
					},
					"stroke_reg_yb1_connectionString": {
						"value": "[parameters('stroke_reg_yb1_connectionString')]"
					},
					"stroke_reg_yb1_password": {
						"value": "[parameters('stroke_reg_yb1_password')]"
					},
					"pmh_azdf_yb2_properties_typeProperties_userName": {
						"value": "[parameters('pmh_azdf_yb2_properties_typeProperties_userName')]"
					},
					"stroke_reg_yb1_properties_typeProperties_userName": {
						"value": "[parameters('stroke_reg_yb1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}